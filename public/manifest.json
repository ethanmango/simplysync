{
    "manifest_version": 3,
    "name": "simplysync",
    "version": "1.0",
    "description": "",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting",
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/overlay.js"]
    }
  ]
  }